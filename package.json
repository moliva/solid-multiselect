    "eslint": "^8.56.0",
    "eslint-plugin-solid": "^0.13.2",
    "nodemon": "^2.0.15",
    "typescript": "^5.1.3"
  },
  "peerDependencies": {
    "solid-js": "^1.8.16"
  },
