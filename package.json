{
  "name": "@moliva/solid-multiselect",
  "version": "0.1.0",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/moliva/solid-multiselect.git"
  },
  "bugs": {
    "url": "https://github.com/moliva/solid-multiselect/issues"
  },
  "homepage": "https://github.com/moliva/solid-multiselect#readme",
  "main": "dist/index.jsx",
  "module": "dist/index.jsx",
  "types": "dist/index.d.ts",
  "sideEffects": "false",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20.0",
    "yarn": ">=1.22.5"
  },
  "keywords": [
    "solid",
    "solidjs",
    "components",
    "multiSelect",
    "dropdown",
